package ch02;

public class ch02_10 {
	public static void main(String args[]){
		System.out.println("15 > 5 is \n"+(15>5));
		System.out.println("15 < 5 is "+(15<5));
		System.out.println("15 >= 5 is "+(15>=5));
		System.out.println("15 <= 5 is "+(15<=5));
		System.out.println("15 != 5 is "+(15!=5));
		System.out.println("15 == 5 is "+(15==5));
	}
}
